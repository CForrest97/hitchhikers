{
  "manifest_version": 2,

  "name": "Modify CSS and HTML",
  "version": "0.1.0",
  "description": "Lateral blog post demo extension",

  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["twitter.js", "hitchhikers.js"],
      "matches": ["https://twitter.com/*"]
    }
  ],
  "permissions": ["https://*/"]
}
